{
  "name": "apigateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "find src -name '*.proto' -type f -exec sh -c 'mkdir -p \"dist/$(dirname \"$1\" | sed \"s|^src/||\")\"; cp \"$1\" \"dist/$(echo \"$1\" | sed \"s|^src/||\")\"' _ {} \\;",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@grpc/grpc-js": "^1.13.2",
    "@grpc/proto-loader": "^0.7.13",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.2",
    "protobufjs": "^7.4.0",
    "socket.io": "^4.8.1",
    "stripe": "^18.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/connect": "^3.4.0",
    "@types/cookie-parser": "^1.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.0",
    "@types/http-errors": "^2.0.0",
    "@types/mime": "^3.0.0",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.12",
    "@types/ms": "^0.7.0",
    "@types/node": "^22.13.13",
    "@types/qs": "^6.9.0",
    "@types/range-parser": "^1.2.0",
    "@types/send": "^0.17.0",
    "@types/serve-static": "^1.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}